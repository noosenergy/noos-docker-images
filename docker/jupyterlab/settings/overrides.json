{
  "@jupyterlab/filebrowser-extension:browser": {
    "navigateToCurrentDirectory": true
  },
  "@jupyterlab/docmanager-extension:plugin": {
    "defaultViewers": {
      "markdown": "Markdown Preview"
    }
  },
  "@jupyterlab/apputils-extension:themes": {
    "theme": "JupyterLab Light"
  },
  "@jupyterlab/git:plugin": {
    "simpleStaging": false,
    "cancelPullMergeConflict": true
  },
  "jupyterlab-code-snippets:snippets": {
    "snippetPreviewFontSize": 8,
    "snippets": [
      {
        "code": [
          "import datetime as dt",
          "import pandas as pd",
          "import numpy as np",
          "from dask import dataframe as dd",
          ""
        ],
        "description": "Import main libraries",
        "id": 0,
        "language": "Python",
        "name": "global_import",
        "tags": [
          "import"
        ]
      },
      {
        "code": [
          "",
          "from IPython.core.interactiveshell import InteractiveShell",
          "InteractiveShell.ast_node_interactivity = \"all\"",
          ""
        ],
        "description": "Enable multiple outputs per cell",
        "id": 2,
        "language": "Python",
        "name": "utils_multicell_output",
        "tags": [
          "utils"
        ]
      },
      {
        "code": [
          "",
          "pd.options.display.max_columns = 30",
          "pd.options.display.max_rows = 100",
          ""
        ],
        "description": "Enable pandas max rows and max columns",
        "id": 3,
        "language": "Python",
        "name": "utils_pandas_options",
        "tags": [
          "utils"
        ]
      }
    ]
  }
}


